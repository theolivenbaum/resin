@model Sir.Search.SearchResult

@{
    var q = Context.Request.Query["q"].ToString();
    ViewBag.Title = (string.IsNullOrWhiteSpace(q) ? "Your personal search engine" : q) + " - CrawlCrawler";
    Layout = "~/Views/_Layout.cshtml";
}
@{await Html.RenderPartialAsync("SearchForm");}

@if (ViewData["time_ms"] != null)
{
    await Html.RenderPartialAsync("DocumentList", Model);
}